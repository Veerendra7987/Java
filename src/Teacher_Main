public class TeacherMain {

    public static void main(String[] args) {
        Teacher s1=new Teacher();
        
        s1.input("Ashu", "English");
        s1.output();
        
        s1.input("Umesh", "Hindi");
        s1.output();
        
        s1.input("Abhay", "Maths");
        s1.output();
    }
    
}
